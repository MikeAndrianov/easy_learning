<div class="contact-form span4">
    <h1> Event list</h1>
      <% current_user.events.where("ends_at>'#{Time.now}'").order("starts_at ASC").each do |event| %>
        <h4>
          <div><%= event.name %></div>
          <div><%= event.description %></div>
          <div><%= event.location %></div>
          <div><%= event.starts_at %></div>
          <div><%= event.ends_at %></div>
        </h4>
      <% end %>
</div>